<div id="keywords" class="cloud">
</div>

{% set tags = get_tag_for_weights() %}
<script>
    $('document').ready(function() {
        var words = [
            {%- for key, value in tags -%}
                {text: "{{ key }}", weight: {{ value.count }}, link:"{{  path('tag_show', {'slug': value.slug}) }}" }
                {%- if not loop.last -%}, {%- endif -%}
            {%- endfor -%}
        ];
        $('#keywords').jQCloud(words);
    });
</script>